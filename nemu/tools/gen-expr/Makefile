# Makefile for building and running the expression generator

# Define the compiler and flags
#CC = gcc
#CFLAGS = -Wall -O2
#
## Define the target executable
#TARGET = expr_gen
#TMP_FILE = /tmp/.code.c
#EXEC_FILE = /tmp/.expr
#
## Default target
#all: $(TARGET)
#
## Build the target executable
#$(TARGET): $(TARGET).c
#	$(CC) $(CFLAGS) -o $(TARGET) $(TARGET).c
#
## Rule to run the program
#run: $(TARGET)
#	./$(TARGET)
#
## Clean up generated files
#clean:
#	rm -f $(TARGET) $(EXEC_FILE) $(TMP_FILE)
#
## Phony targets
#.PHONY: all run clean
#
#include $(NEMU_HOME)/scripts/build.mk
